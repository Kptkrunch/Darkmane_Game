        x += sign(hsp); 
    } 
    hsp = 0;
    
    dir *= -1;
    image_xscale *= -1;
}
x += hsp;

// Verticle collision, also fear of heights-----

if (place_meeting(x, y+vsp, par_wall))
{
    while(!place_meeting(x, y+sign(vsp), par_wall))
    {
        y += sign(vsp);    
    }
    vsp = 0;

}
y += vsp;



